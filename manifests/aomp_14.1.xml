<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<!-- Manifest for AOMP 14.1-x which will use release branches of external reositories -->

    <remote name="gerritgit" review="git.amd.com:8080" fetch="ssh://gerritgit/" />
    <default revision="release/rocm-rel-4.3" remote="gerritgit" sync-j="4" sync-c="true" />
    <remote name="roctools"  fetch="https://github.com/ROCm-Developer-Tools/" />
    <remote name="roc"  fetch="https://github.com/RadeonOpenCompute/" />

<project remote="roc" path="llvm-project" name="llvm-project"                 revision="amd-stg-open" groups="unlocked" />

<project remote="roc" path="rocm-compilersupport" name="ROCm-CompilerSupport" revision="amd-stg-open" groups="unlocked" />
<project remote="roc" path="rocm-device-libs" name="ROCm-Device-Libs"         revision="amd-stg-open"  groups="unlocked" />
<project remote="roctools" path="flang" name="flang"               revision="aomp-dev" groups="unlocked" />
<project remote="roctools" path="aomp-extras" name="aomp-extras"   revision="aomp-dev" groups="unlocked" />
<project remote="roctools" path="aomp" name="aomp"                 revision="aomp-dev" groups="unlocked" />

<! -- Change these all to 4.5 when ROCm 4.5 sources are externally available -->
<project remote="roctools" path="rocprofiler" name="rocprofiler"              revision="rocm-4.3.x" groups="unlocked" />
<project remote="roctools" path="roctracer" name="roctracer"                  revision="rocm-4.3.x" groups="unlocked" />
<project remote="roctools" path="ROCdbgapi" name="ROCdbgapi"                  revision="rocm-4.3.x" groups="unlocked" />
<project remote="roctools" path="ROCgdb" name="ROCgdb"                        revision="rocm-4.3.x" groups="unlocked" />
<project remote="roctools" path="hipamd" name="hipamd"                        revision="develop" groups="unlocked" />
<project remote="roctools" path="hip"    name="hip"                           revision="develop" groups="unlocked" />
<project remote="roctools" path="ROCclr" name="ROCclr"                        revision="develop" groups="unlocked" />
<project remote="roc" path="ROCm-OpenCL-Runtime" name="ROCm-OpenCL-Runtime"   revision="develop" groups="unlocked" />
<project remote="roc" path="rocminfo" name="rocminfo"                         revision="roc-4.3.x" groups="unlocked" />
<project remote="roc" path="rocm-cmake" name="rocm-cmake"                     revision="roc-4.3.x" groups="unlocked" />
<project remote="roc" path="rocr-runtime" name="ROCR-Runtime"                 revision="rocm-4.3.x" groups="unlocked" />
<project remote="roc" path="roct-thunk-interface" name="ROCT-Thunk-Interface" revision="roc-4.3.x" groups="unlocked" />
</manifest>
