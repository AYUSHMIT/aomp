<?xml version="1.0" encoding="UTF-8"?>
<manifest>
<!-- Manifest for AOMP 14.0-x which uses internal repos for non-aomp components -->
    <remote name="gerritgit" review="git.amd.com:8080" fetch="ssh://gerritgit/" />
    <default revision="release/rocm-rel-4.3" remote="gerritgit" sync-j="4" sync-c="true" />
    <remote name="roctools"  fetch="https://github.com/ROCm-Developer-Tools/" />
    <remote name="roc"  fetch="https://github.com/RadeonOpenCompute/" />

<!-- INTERNAL REPOS -->
<!-- ROCr ROCt -->
<project remote="gerritgit" path="hsa-runtime" name="hsa/ec/hsa-runtime"            revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="roct-thunk-interface" name="compute/ec/libhsakmt" revision="release/rocm-rel-4.5" groups="unlocked" />

<!-- Lightning -->
<project remote="gerritgit" path="rocm-compilersupport" name="lightning/ec/support" revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="rocm-device-libs" name="lightning/ec/device-libs" revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="llvm-project" name="lightning/ec/llvm-project"    revision="amd-stg-open" groups="unlocked" />

<!-- ROCm compute -->
<project remote="gerritgit" path="rocminfo" name="compute/ec/rocminfo"              revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="ROCgdb" name="compute/ec/rocm-gdb"                revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="ROCdbgapi" name="compute/ec/rocm-dbgapi"          revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="rocprofiler" name="compute/ec/rocprofiler"        revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="roctracer" name="compute/ec/roctracer"            revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="hipamd" name="compute/ec/hipamd"                  revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="hip" name="compute/ec/hip"                        revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="ROCclr" name="compute/ec/vdi"                     revision="release/rocm-rel-4.5" groups="unlocked" />
<project remote="gerritgit" path="rocm-cmake" name="compute/ec/packaging/rocm-cmake" revision="release/rocm-rel-4.5" groups="unlocked" />

<!-- HIP/ROCclr/OpenCL -->
<project remote="gerritgit" path="ROCm-OpenCL-Runtime" name="brahma/ec/packaging/rocm-opencl" revision="amd-staging" groups="unlocked" />

<!-- EXTERNAL REPOS -->
<project remote="roctools" path="flang" name="flang"               revision="aomp-dev" groups="unlocked" />
<project remote="roctools" path="aomp-extras" name="aomp-extras"   revision="aomp-dev" groups="unlocked" />
<project remote="roctools" path="aomp" name="aomp"                 revision="aomp-dev" groups="unlocked" />
</manifest>
