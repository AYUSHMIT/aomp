                                uint32_t *);
void __kmpc_xteamr_max_l_16x64(long int, long int *, long int *, uint32_t *);
void __kmpc_xteamr_max_ul_16x64(unsigned long, unsigned long *, unsigned long *,
                                uint32_t *);
